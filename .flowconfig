[ignore]
<PROJECT_ROOT>/node_modules/.cache
<PROJECT_ROOT>/node_modules/map-or-similar
<PROJECT_ROOT>/node_modules/memoizerific

[untyped]
<PROJECT_ROOT>/node_modules/react-beautiful-dnd
<PROJECT_ROOT>/node_modules/date-fns-tz
<PROJECT_ROOT>/node_modules/unstated
<PROJECT_ROOT>/node_modules/graphql
<PROJECT_ROOT>/src/generated

[include]

[libs]

[lints]

[options]
sharedmemory.hash_table_pow=21
lazy_mode=fs
name= Zenport web app
module.file_ext=.js
module.file_ext=.mjs
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.graphql
module.name_mapper.extension='graphql' -> 'empty/object'
module.name_mapper='^apollo$' -> '<PROJECT_ROOT>/src/apollo.js'
module.name_mapper='^serviceWorker$' -> '<PROJECT_ROOT>/src/serviceWorker.js'
module.name_mapper='^graphql\(.*\)$' -> '<PROJECT_ROOT>/src/graphql/\1'
module.name_mapper='^hoc\(.*\)$' -> '<PROJECT_ROOT>/src/hoc/\1'
module.name_mapper='^i18n\(.*\)$' -> '<PROJECT_ROOT>/src/i18n/\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^hooks\(.*\)$' -> '<PROJECT_ROOT>/src/hooks/\1'
module.name_mapper='^constants\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^modules\(.*\)$' -> '<PROJECT_ROOT>/src/modules/\1'
module.name_mapper='^generated\(.*\)$' -> '<PROJECT_ROOT>/src/generated/\1'
module.name_mapper='^media\(.*\)$' -> '<PROJECT_ROOT>/src/media/\1'
module.name_mapper='^providers\(.*\)$' -> '<PROJECT_ROOT>/src/providers/\1'
module.name_mapper='^styles\(.*\)$' -> '<PROJECT_ROOT>/src/styles/\1'
module.name_mapper='^types\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
esproposal.optional_chaining=enable
esproposal.nullish_coalescing=enable
[strict]
