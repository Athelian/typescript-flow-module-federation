[ignore]
<PROJECT_ROOT>/node_modules
[include]

[libs]

[lints]

[options]
module.name_mapper='^i18n\(.*\)$' -> '<PROJECT_ROOT>/src/i18n/\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^constants\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^modules\(.*\)$' -> '<PROJECT_ROOT>/src/modules/\1'
module.name_mapper='^media\(.*\)$' -> '<PROJECT_ROOT>/src/media/\1'
module.name_mapper='^styles\(.*\)$' -> '<PROJECT_ROOT>/src/styles/\1'
module.name_mapper='^types\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.file_ext=.graphql
module.name_mapper.extension='graphql' -> 'empty/object'
[strict]
