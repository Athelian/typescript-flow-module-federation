  "scripts": {
    "start": "node scripts/start.js",
    "build": "build:prod",
    "build:prod": "cross-env NODE_ENV=production node scripts/build.js",
    "build:stg": "cross-env NODE_ENV=staging node scripts/build.js",
    "now-build": "build:stg",
    "postbuild": "react-snap",
    "test": "node scripts/test.js",
    "stylelint": "stylelint src/**/*.{js,jsx,css}",
    "e2e": "npm-run-all cy:install cy:open",
    "cy:open": "npx cypress open",
    "cy:install": "npx cypress install",
    "cy:record": "npx cypress run --record --key 5fa121a7-1994-41fc-963a-9c4fbe7ab112",
    "post:install": "npm run flow-typed && npm run apollo-fragments",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.{js,jsx}' -t html -t json -t text --threshold 80",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "commit": "npx git-cz",
    "flow-typed": "rimraf flow-typed/npm && npx flow-typed install --ignoreDeps dev bundled peer",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 node scripts/build.js",
    "apollo-fragments": "node scripts/apolloFragment.js",
    "translate:prepare": "cross-env NODE_ENV=production babel-node --no-babelrc --presets=@babel/preset-env scripts/migrate.js",
    "translate:clean": "rimraf node_modules/.cache/",
    "translate": "npm-run-all translate:clean build:stg translate:generate",
    "translate:generate": "cross-env NODE_ENV=production babel-node --no-babelrc --presets=@babel/preset-env scripts/translate.js",
    "check:quality": "npx jscpd src",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "gql-gen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx,json}": [
        "eslint --fix",
        "prettier-eslint --write",
        "git add"
      ],
      "*.md": [
        "prettier-eslint --write",
        "git add"
      ]
    },
    "ignore": [
      "**/generated/**/*.{js,json}"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/node": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
