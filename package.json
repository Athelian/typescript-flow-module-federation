  "scripts": {
    "start": "node scripts/start.js",
    "build": "build:prod",
    "build:prod": "cross-env NODE_ENV=production node scripts/build.js",
    "build:stg": "cross-env NODE_ENV=staging node scripts/build.js",
    "postbuild": "react-snap",
    "test": "node scripts/test.js",
    "stylelint": "stylelint src/**/*.{js,jsx,css}",
    "cy:open": "npx cypress open",
    "cy:install": "npx cypress install",
    "cy:record": "npx cypress run --record --key 5fa121a7-1994-41fc-963a-9c4fbe7ab112",
    "cy:headed": "npx cypress run --headed",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "e2e-no-ui": "start-server-and-test start http://localhost:3000 cy:headed",
    "e2e-record": "start-server-and-test start http://localhost:3000 cy:record",
    "post:install": "npm run flow-typed && npm run apollo-fragments",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.{js,jsx}' -t html -t json -t text --threshold 80",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "commit": "npx git-cz",
    "flow-typed": "rimraf flow-typed/npm && npx flow-typed install --ignoreDeps dev bundle peer",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 node scripts/build.js",
    "apollo-fragments": "node scripts/apolloFragment.js",
    "translate:prepare": "cross-env NODE_ENV=production babel-node --no-babelrc --presets=@babel/preset-env scripts/migrate.js",
    "translate": "cross-env NODE_ENV=production babel-node --no-babelrc --presets=@babel/preset-env scripts/translate.js",
    "check:quality": "npx jsinspect -t 50 src",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "styleguide": "styleguidist server",
    "build:styleguide": "styleguidist build",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ],
    "*.md": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@storybook/addon-a11y": "^4.0.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "@svgr/webpack": "^3.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-preset-react-app": "^6.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "chance": "^1.0.16",
    "cross-env": "^5.2.0",
    "css-loader": "1.0.1",
