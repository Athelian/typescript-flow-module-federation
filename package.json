    "downshift": "^2.0.14",
    "emotion": "^9.2.6",
    "fontfaceobserver": "^2.0.13",
    "formik": "^1.0.2",
    "graphql": "0.13.2",
    "graphql-tag": "^2.9.2",
    "js-cookie": "^2.2.0",
    "loglevel": "^1.6.1",
    "ramda": "^0.25.0",
    "raven-js": "^3.26.4",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.6",
    "react-fullstory": "^1.0.3",
    "react-hot-loader": "^4.3.3",
    "react-intercom": "^1.0.14",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "yup": "^0.26.0",
    "zenform": "^0.0.0-beta.9"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "cy:open": "cypress open",
    "cy:install": "cypress install",
    "e2e": "start-server-and-test start http://localhost:3000 cy:open",
    "postinstall": "sh ./postinstall.sh",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.{js,jsx}' -t html -t json -t text --threshold 80",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx}'",
    "precommit": "npm run flow && lint-staged",
    "commit": "npx git-cz",
    "commitmsg": "commitlint -e",
    "flow-typed": "rimraf flow-typed/npm && npx flow-typed install --ignoreDeps dev bundle peer",
    "build:stg": "cross-env NODE_ENV=staging node scripts/build.js",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 node scripts/build.js",
    "apollo-fragments": "node scripts/apolloFragment.js",
    "check:translations": "node scripts/checkTranslations.js",
    "check:quality": "npx jsinspect -t 50 src",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "styleguide": "styleguidist server",
    "build:styleguide": "styleguidist server"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ],
    "*.md": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-flow": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "@babel/runtime": "7.0.0-beta.46",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@storybook/addon-a11y": "^3.4.8",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "autoprefixer": "8.6.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-emotion": "^9.2.5",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-named-asset-import": "1.0.0-next.3e165448",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "4.0.0-next.3e165448",
    "babel-runtime": "^6.26.0",
    "bfj": "5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.11",
    "cypress": "^3.0.2",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "3.0.0-next.3e165448",
    "eslint-loader": "2.0.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "1.1.11",
