{
  "hosting": [
    {
      "target": "staging",
      "public": "build",
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache, no-store, must-revalidate"
            },
            {
              "key": "X-Frame-Options",
              "value": "SAMEORIGIN"
            },
            {
              "key": "X-Content-Type-Options",
              "value": "nosniff"
            },
            {
              "key": "Content-Security-Policy-Report-Only",
              "value": "script-src 'self' https://polyfill.io;"
            },
            {
              "key": "Referrer-Policy",
              "value": "same-origin"
            },
            {
              "key": "Feature-Policy",
              "value": "vibrate 'none'; microphone 'none'; camera 'none'; magnetometer 'none'; gyroscope 'none'; speaker 'none'; payment 'none' https://staging.zenport.io"
            }
          ]
        },
        {
          "source": "**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|eot|otf|ttf|ttc|woff|woff2|font.css)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=604800"
            }
          ],
          "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
        },
        {
          "source": "/service-worker.js",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"
            }
          ]
        },
        {
          "source": "/index.html",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            }
          ]
        },
        {
          "source": "/new/shipment/table.html",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            }
          ]
        },
        {
          "source": "/new/project/table.html",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "/favicon.ico",
          "destination": "/favicon.ico"
        },
        {
          "source": "/new/shipment/table",
          "destination": "/new/shipment/table.html"
        },
        {
          "source": "/new/project/table",
          "destination": "/new/project/table.html"
        },
        {
          "source": "/public/**",
          "function": "proxyStagingFile"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    },
