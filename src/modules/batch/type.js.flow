// @flow

export type MetricValue = {
  value: number,
  metric: string,
};

export type BatchCreate = {
  no: string,
  quantity: number,
  orderItemId?: ?string,
  shipmentId?: ?string,
  producedAt?: ?string,
  deliveredAt?: ?string,
  expiredAt?: ?string,
  packageQuantity?: ?number,
  packageName?: ?string,
  packageGrossWeight?: ?MetricValue,
  packageVolume?: ?MetricValue,
  packageSize?: ?{
    length: MetricValue,
    width: MetricValue,
    height: MetricValue,
  },
  packageCapacity?: ?number,
  tagIds?: ?Array<string>,
};

export type BatchUpdate = {
  archived?: ?boolean,
  no?: ?string,
  quantity?: ?number,
  shipmentId?: ?string,
  producedAt?: ?string,
  deliveredAt?: ?string,
  expiredAt?: ?string,
  packageQuantity?: ?number,
  packageName?: ?string,
  packageGrossWeight?: ?MetricValue,
  packageVolume?: ?MetricValue,
  packageSize?: ?{
    length: MetricValue,
    width: MetricValue,
    height: MetricValue,
  },
  packageCapacity?: ?number,
  tagIds?: ?Array<string>,
  orderItemId?: ?string,
};
